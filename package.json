{
  "name": "ember-webrtc-devices",
  "version": "2.0.1",
  "description": "Enumerating and selecting media devices from the browser as an ember addon",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "semistandard addon/**/*.js app/**/*.js blueprints/**/*.js tests/**/*.js && ember test",
    "deploy:gh-pages": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -"
  },
  "repository": "https://github.com/MyPureCloud/ember-webrtc-devices/",
  "engines": {
    "node": ">= 4.4.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-ajax": "0.7.1",
    "ember-cli": "2.3.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-github-pages": "0.0.8",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.2.1",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^2.0.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.3.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-intl": "^2.4.1",
    "ember-load-initializers": "^0.5.0",
    "ember-resolver": "^2.0.3",
    "ember-truth-helpers": "^1.2.0",
    "ember-try": "^0.1.2",
    "emberx-select": "2.1.0",
    "loader.js": "^4.0.0",
    "pre-push": "^0.1.1",
    "semistandard": "^7.0.5"
  },
  "keywords": [
    "ember-addon",
    "webrtc",
    "webrtc-devices",
    "webcam"
  ],
  "dependencies": {
    "broccoli-json-module": "0.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-source": "^1.1.0",
    "ember-cli-babel": "^5.1.5",
    "ember-cli-htmlbars": "^1.0.3"
  },
  "pre-push": [
    "test"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
